  name        String   @map("name") @database.Text
  deletedTime DateTime @default(now()) @map("deleted_time")
  isDeleted   Boolean  @default(false) @map("is_deleted")

  @@map("VisaPurpose")
}

model VisaType {
  id          Int      @id @map("id") @default(autoincrement())
  name        String   @map("name") @database.Text
  deletedTime DateTime @default(now()) @map("deleted_time")
  isDeleted   Boolean  @default(false) @map("is_deleted")

  @@map("VisaType")
}

model VisaVietnamArrival {
  id          Int      @id @map("id") @default(autoincrement())
  countryId   Int?     @map("country_id")
  dayFree     Int?     @map("day_free")
  deletedTime DateTime @default(now()) @map("deleted_time")
  isDeleted   Boolean  @default(false) @map("is_deleted")

  @@index([countryId], map: "fk_visa_vietnam_arrival_country1")
  @@map("VisaVietnamArrival")
}

model Voucher {
  id             Int      @id @map("id") @default(autoincrement())
  name           String?  @map("name") @database.VarChar(255)
  voucherCode    String   @map("voucher_code") @database.VarChar(255)
  customerId     Int?     @map("customer_id")
  googleClientId Int?     @map("google_client_id")
  voucherTypeId  Int?     @map("voucher_type_id")
  orderId        Int?     @map("order_id")
  isActive       Boolean  @map("is_active") @default(false)
  isUsed         Boolean  @map("is_used") @default(false)
  isRunning      Boolean  @map("is_running") @default(false)
  expiredTime    DateTime @map("expired_time") @database.DateTime(0)
  activeDate     DateTime @map("active_date") @database.DateTime(0)
  usedDate       DateTime @map("used_date") @database.DateTime(0)
  openDate       DateTime @map("open_date") @database.DateTime(0)
  createdTime    DateTime @default(now()) @map("created_time")
  updatedTime    DateTime @updatedAt @map("updated_time")
  deletedTime    DateTime @default(now()) @map("deleted_time")
  isDeleted      Boolean  @default(false) @map("is_deleted")

  @@index([customerId], map: "fk_vou_customer_id_cus_id_7f7y")
  @@index([orderId], map: "fk_vou_order_id_ord_id_8rjw")
  @@index([voucherTypeId], map: "fk_vou_voucher_type_id_vou_id_bgsy")
  @@map("Voucher")
}

model VoucherType {
  id          Int      @id @map("id") @default(autoincrement())
  name        String   @map("name") @database.VarChar(225)
  config      Json?    @map("config")
  percent     Int?     @map("percent")
  uuid        String   @map("uuid") @database.VarChar(36)
  createdTime DateTime @default(now()) @map("created_time")
  updatedTime DateTime @updatedAt @map("updated_time")
  deletedTime DateTime @default(now()) @map("deleted_time")
  isDeleted   Boolean  @default(false) @map("is_deleted")

  @@map("VoucherType")
}

model Website {
  id                       Int      @id @map("id") @default(autoincrement())
  currencyId               Int      @map("currency_id") @default(1)
  name                     String   @map("name") @database.VarChar(100)
  domain                   String?  @map("domain") @database.VarChar(100)
  domainDashboard          String?  @map("domain_dashboard") @database.VarChar(100)
  stripeKey                String?  @map("stripe_key") @database.Text
  paypalKey                String?  @map("paypal_key") @database.Text
  mailchimpKey             String?  @map("mailchimp_key") @database.Text
  minAmount                Float    @map("min_amount") @default(0) @database.Float
  minUnit                  Float    @map("min_unit") @default(0) @database.Float
  email                    String?  @map("email") @database.Text
  emailContact             String?  @map("email_contact") @database.VarChar(100)
  emailName                String?  @map("email_name") @database.Text
  emailCode                String?  @map("email_code") @database.Text
  emailCredential          String?  @map("email_credential") @database.Text
  companiesHouseCredential String?  @map("companies_house_credential") @database.Text
  analyticsSignature       String?  @map("analytics_signature") @database.VarChar(3072)
  viewId                   String?  @map("view_id") @database.VarChar(32)
  emailToken               String?  @map("email_token") @database.Text
  tawkToWebhook            String?  @map("tawk_to_webhook") @database.VarChar(255)
  awsBucketKey             String?  @map("aws_bucket_key") @database.Text
  xeroTenantName           String?  @map("xero_tenant_name") @database.VarChar(100)
  xeroTenantId             String?  @map("xero_tenant_id") @database.VarChar(100)
  xeroClientId             String?  @map("xero_client_id") @database.VarChar(100)
  xeroClientSecret         String?  @map("xero_client_secret") @database.VarChar(100)
  xeroHookKey              String?  @map("xero_hook_key") @database.VarChar(100)
  xeroRedirectUrl          String?  @map("xero_redirect_url") @database.VarChar(100)
  awsBucketLog             String?  @map("aws_bucket_log") @database.Text
  googleAdsConfig          String?  @map("google_ads_config") @database.Text
  endpointSecretStripe     String?  @map("endpoint_secret_stripe") @database.Text
  minMakePaymentAmount     Float    @map("min_make_payment_amount") @default(0) @database.Float
  createdTime              DateTime @default(now()) @map("created_time")
  deletedTime              DateTime @default(now()) @map("deleted_time")
  isDeleted                Boolean  @default(false) @map("is_deleted")

  @@index([currencyId], map: "fk_website_currency_1_idx")
  @@map("Website")
}

model XeroAccount {
  id          Int      @id @map("id") @default(autoincrement())
  name        String   @map("name") @database.VarChar(50)
  description String?  @map("description") @database.VarChar(200)
  type        String   @map("type") @default("SALE") @database.VarChar(50)
  code        String   @map("code") @database.VarChar(50)
  createdTime DateTime @default(now()) @map("created_time")
  updatedTime DateTime @updatedAt @map("updated_time")
  deletedTime DateTime @default(now()) @map("deleted_time")
  isDeleted   Boolean  @default(false) @map("is_deleted")

  @@map("XeroAccount")
}

model XeroInvoiceStatus {
  id          Int      @id @map("id") @default(autoincrement())
  name        String   @map("name") @database.VarChar(100)
  xeroCode    String   @map("xero_code") @database.VarChar(100)
  description String?  @map("description") @database.VarChar(200)
  deletedTime DateTime @default(now()) @map("deleted_time")
  isDeleted   Boolean  @default(false) @map("is_deleted")

  @@map("XeroInvoiceStatus")
}
